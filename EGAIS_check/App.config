        </setting>
        <setting name="LastRestsDateTime" serializeAs="String">
            <value />
        </setting>
        <setting name="LastPartnerQueryID" serializeAs="String">
            <value />
        </setting>
        <setting name="CurrentSettings" serializeAs="String">
            <value>2000</value>
        </setting>
        <setting name="UTM_hosts_list" serializeAs="Xml">
